{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/",
            "methods": ["POST", "GET", "PUT", "DELETE"],
            "headers" : {
                "Access-Control-Allow-Origin": "https://cook-io-mu.vercel.app"
            }
        }
    ],
    "headers": [
        {
            "source": "/api/*",
            "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" }
            ]
        }
    ]
}