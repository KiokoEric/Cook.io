{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/",
            "methods": ["POST", "GET", "PUT", "DELETE"],
            "headers": [
                { "key": "Access-Control-Allow-Origin", "value": "https://cook-io-server.vercel.app" }
            ]
        }
    ]
}